#question 1
for number in range(1,21):
    if number % 2!=0:
        continue
    if number == 10:
        continue
    print(number)
    
#question 2   
sum=0
while True:
    number=float(input("Enter a number(negative number to stop):"))
    if number<0:
        break
    sum +=number
print(f"The sum of all positive numbers entered is:{sum}")

#question 3
con=0
num=1
while con<5:
    mux=num*3
    if mux%4==0:
        num+=1
        continue
    print(mux)
    con +=1
    num +=1
    
